{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node":true,
        "jest":true,
        "phantomjs": true
    },
    "extends": "fbjs",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "indent": [
            "error",
            4
        ],
        "linebreak-style": [
            "error",
            "unix"
        ]
    }
}